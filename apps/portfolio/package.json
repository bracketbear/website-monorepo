{
  "name": "@bracketbear/portfolio",
  "type": "module",
  "version": "1.0.1",
  "description": "Personal portfolio website showcasing projects, work history, and skills",
  "scripts": {
    "dev": "node ../../scripts/generate-repo-map.js && astro dev --port 4322",
    "prebuild": "node ../../scripts/generate-repo-map.js && node ../../scripts/update-main-readme.js",
    "build": "astro build",
    "preview": "astro preview --port 4322",
    "deploy": "netlify deploy --prod",
    "astro": "astro",
    "type-check": "tsc --noEmit",
    "update-job-skills": "node scripts/update-job-skills.js",
    "sync": "npx astro sync"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@bracketbear/astro-content": "file:../../packages/astro-content",
    "@bracketbear/bear-ui-react": "file:../../packages/bear-ui-react",
    "@bracketbear/core": "file:../../packages/core",
    "@bracketbear/flateralus": "file:../../packages/flateralus",
    "@bracketbear/flateralus-p5": "file:../../packages/flateralus-p5",
    "@bracketbear/flateralus-p5-animations": "file:../../packages/flateralus-p5-animations",
    "@bracketbear/flateralus-pixi": "file:../../packages/flateralus-pixi",
    "@bracketbear/flateralus-pixi-animations": "file:../../packages/flateralus-pixi-animations",
    "@bracketbear/flateralus-react": "file:../../packages/flateralus-react",
    "astro": "^5.10.1",
    "marked": "^16.1.1",
    "pixi.js": "^8.11.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@types/marked": "^5.0.2",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "vite-plugin-svgr": "^4.3.0"
  }
}
